<div id="setup_box">
<%= form_for @category, :html => {:multipart => true} do |f| %>
  <%= render :partial => "layouts/error_block", :locals => {:object => @category} %>

  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :url_path %><br />
    <%= f.text_field :url_path %>
  </div>
  <div class="field">
    <%= f.label :list_order, "List Order" %><br />
    <%= f.text_field :list_order, :size => "30x10" %>
  </div>
  <div class="field">
    <%= f.label :image, "Image Upload" %>
    <%= f.file_field :image %>
  </div>
  <div class="clear"></div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description, :cols => 65, :rows => 10 %>
  </div>
  <div class="clear"></div>
  <div class="field">
    <%= f.submit "Create", :class => "button_lg orange_button" %>
  </div>
  <div class="field">
    <%= link_to 'Cancel', admin_dashboard_path, :class => "button_lg orange_button"%>
  </div>
<% end %>
</div>
